DEFINES        += USE_WGS

include(wgsconfig.pri)

linux-g++ {
  WGS_BASE     = /home/bacon/Frameworks/libwgs
  INCLUDEPATH  += $${WGS_BASE}/include/libwgs
  LIBS         += -L$${WGS_BASE}/lib -lwgs
}

win32 {
  WGS_BASE     = $${WGS_INSTALL_PREFIX}
  INCLUDEPATH  += $${WGS_BASE}/include/libwgs
  LIBS         += -L$${WGS_BASE}/lib libwgs.lib
}

